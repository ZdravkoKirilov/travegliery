{
  "name": "goatzilla",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run generate-icons && ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "generate-icons": "svg-generator",
    "serve": "http-server dist/goatzilla",
    "deploy": "npm run build && ng deploy --base-href=/goatzilla/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.1.1",
    "@angular/common": "~12.1.1",
    "@angular/compiler": "~12.1.1",
    "@angular/core": "~12.1.1",
    "@angular/forms": "~12.1.1",
    "@angular/platform-browser": "~12.1.1",
    "@angular/platform-browser-dynamic": "~12.1.1",
    "@angular/router": "~12.1.1",
    "@ngneat/svg-icon": "^3.2.0",
    "date-fns": "^2.22.1",
    "ng-inline-svg": "^13.0.0",
    "normalize.css": "^8.0.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.1.1",
    "@angular/cli": "~12.1.1",
    "@angular/compiler-cli": "~12.1.1",
    "@ngneat/svg-generator": "^3.0.1",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "^1.0.0-rc.2",
    "http-server": "^0.12.3",
    "jasmine-core": "~3.7.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "typescript": "~4.3.2"
  },
  "svgGenerator": {
    "outputPath": "./src/assets/generated-icons",
    "prefix": "app",
    "srcPath": "./src/assets/svg-icons",
    "svgoConfig": {
      "plugins": [
        "removeDimensions"
      ]
    }
  }
}
